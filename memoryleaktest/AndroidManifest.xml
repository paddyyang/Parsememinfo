<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pyzed.memoryleaktest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
			android:name=".MainActivity"
			android:label="@string/app_name"  >
			<intent-filter>
				<action  android:name="android.intent.action.MAIN"  />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
        <activity
			android:name=".TestEventListener"
			android:label="@string/app_name"  >
		</activity>
        <activity
			android:name=".TestLocation"
			android:label="@string/app_name"  >
		</activity>
        <activity
			android:name=".TestLoadBitmap"
			android:label="@string/app_name"  >
		</activity>
        <activity
			android:name=".TestStaticActivity"
			android:label="@string/app_name"  >
		</activity>
        <activity
			android:name=".TestAnonyClass"
			android:label="@string/app_name"  >
		</activity>
        <activity
			android:name=".TestSensor"
			android:label="@string/button_test_sensor"  >
		</activity>
    </application>
</manifest>
